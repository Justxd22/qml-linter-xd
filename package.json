    "eslint": "^9.0.0",
    "eslint-doc-generator": "^1.0.0",
    "eslint-plugin-eslint-plugin": "^6.0.0",
    "eslint-plugin-n": "^17.0.0",
    "mocha": "^10.0.0",
    "npm-run-all2": "^6.1.2"
  },
  "engines": {
    "node": "^18.18.0 || ^20.9.0 || >=21.1.0"
  },
  "peerDependencies": {
    "eslint": ">=8.57.0"
  },
  "license": "GPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "justxd22/qml-linter-xd"
  },
  "bugs": {
    "url": "https://github.com/justxd22/qml-linter-xd/issues"
